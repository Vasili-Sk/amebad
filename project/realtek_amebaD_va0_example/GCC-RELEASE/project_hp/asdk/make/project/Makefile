include $(MAKE_INCLUDE_GEN)

.PHONY: all clean

#*****************************************************************************#
#                               VARIABLES	                              #
#*****************************************************************************#

#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#
all:
	@$(MAKE) -C xip all
	@$(MAKE) -C sram all
	@$(MAKE) -C library all
	@$(MAKE) -C src all

#*****************************************************************************#
#              CLEAN GENERATED FILES                                          #
#*****************************************************************************#
clean:
	@$(MAKE) -C xip clean
	@$(MAKE) -C sram clean
	@$(MAKE) -C library clean
	@$(MAKE) -C src clean

-include $(DEPS)
